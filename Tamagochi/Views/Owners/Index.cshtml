@{
  Layout = "_Layout";
}

<h1>Owners</h1>
<h3>Click on the Owner's name to add a Pet</h3>

@if (@Model.Count == 0)
{
  <h3>No Owners have been added yet!</h3>
}

@foreach (var owner in Model)
{
  <h3><a href='/owners/@owner.GetId()'>@owner.GetName()</a></h3><br><br>
}

<p><a href='/'>Back home</a></p>
